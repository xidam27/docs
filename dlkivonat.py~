import requests

url = 'https://ibank.takarekpont.hu/eib/login'
data = 'pageid=6BEyHjjDGjER5RN0vEigfDajZVUnWeJXRHhZPoG&lang=hu&cgcode=&group=&user=dedico2013&uid=dedico2013&pwd=q1w2e3r4&B1=Bejelentkez%E9s'

def data2dict(data):
    newdata = dict()
    data = data.split("&")
    for value in data:
        key, value = value.split("=")
        newdata.update({key: value})
    return newdata
        
values = data2dict(data)

r = requests.get(url)
jar = r.cookies
r = requests.post(url, data=values, cookies=jar)

print r.content


